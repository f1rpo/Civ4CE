	<ElementType name="Time" content="textOnly" dt:type="float"/>
	<ElementType name="Value" content="textOnly" dt:type="float"/>
	<ElementType name="r" content="textOnly" dt:type="float"/>
	<ElementType name="g" content="textOnly" dt:type="float"/>
