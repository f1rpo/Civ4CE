		<element type="bSticky" minOccurs="0"/>
		<element type="bIsProjectile"/>
		<element type="fSpeed" minOccurs="0"/>
		<element type="fArcValue" minOccurs="0"/>
	</ElementType>
	<ElementType name="AttachableInfo" content="eltOnly">
		<element type="Type"/>
		<element type="Description"/>
		<element type="fScale"/>
		<element type="Path"/>
	</ElementType>
